<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.databaseassng">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:configChanges="orientation|keyboardHidden|screenSize"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StoreActivity" />

        <activity android:name=".InternalActivity" />
        <activity android:name=".ExternalActivity" />
        <activity android:name=".SqliteActivity" />
        <activity android:name=".SqliteTable" />
        <activity android:name=".SqliteTableDatabase" />
        <activity android:name=".Table_Register_Activity" />
        <activity android:name=".Table_Login_Activity" />
        <activity android:name=".Table_Update_Activity" />
        <activity android:name=".Table_Delete_Activity" />
        <activity android:name=".Delete_User" />
        <activity android:name=".Update_User" />







    </application>

</manifest>